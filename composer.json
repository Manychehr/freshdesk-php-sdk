{
    "name": "alleotech/freshdesk-php-sdk",
    "description": "PHP SDK for the Freshdesk API (v2)",
    "type": "library",
    "homepage": "https://github.com/alleotech/freshdesk-php-sdk",
    "keywords": ["freshdesk", "customer service", "support", "help desk", "api"],
    "license": "MIT",
    "authors": [
		{
			"name": "AlleoTech Ltd",
			"email": "social@alleo.tech",
			"homepage": "https://alleo.tech"
		},
        {
            "name": "Matthew Clarkson",
            "email": "mpclarkson@hilenium.com",
            "homepage": "http://mpclarkson.github.io/"
        }
    ],
    "require": {
        "php": ">=5.5.0",
        "guzzlehttp/guzzle": ">=4.0,<7.0"
    },
    "require-dev": {
        "evert/phpdoc-md" : "^0.2",
        "squizlabs/php_codesniffer": "*",
        "phpunit/phpunit": "^6.0"
    },
	"replace": {
		"mpclarkson/freshdesk-php-sdk": "<=0.3.1"
	},
    "autoload": {
        "psr-4": {"Freshdesk\\": "src/"}
    },
    "autoload-dev": {
        "psr-4": { "Freshdesk\\tests\\": "tests/" }
    }
}
